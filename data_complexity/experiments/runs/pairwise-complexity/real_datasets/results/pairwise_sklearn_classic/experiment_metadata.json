{
  "experiment_name": "pairwise_sklearn_classic",
  "datasets": [
    {
      "label": "Iris",
      "type": "Iris",
      "params": {
        "train_size": 0.5
      }
    },
    {
      "label": "Wine",
      "type": "Wine",
      "params": {
        "train_size": 0.5
      }
    },
    {
      "label": "Breast Cancer",
      "type": "Breast Cancer",
      "params": {
        "train_size": 0.5
      }
    }
  ],
  "x_label": "Dataset",
  "ml_models": [
    {
      "name": "LogisticRegression",
      "class": "LogisticRegressionModel",
      "parameters": {
        "random_state": 42,
        "scale_features": true,
        "max_iter": 1000
      }
    },
    {
      "name": "KNN-5",
      "class": "KNNModel",
      "parameters": {
        "random_state": 42,
        "scale_features": true,
        "n_neighbors": 5
      }
    },
    {
      "name": "KNN-3",
      "class": "KNNModel",
      "parameters": {
        "random_state": 42,
        "scale_features": true,
        "n_neighbors": 3
      }
    },
    {
      "name": "DecisionTree",
      "class": "DecisionTreeModel",
      "parameters": {
        "random_state": 42,
        "scale_features": false,
        "max_depth": 5
      }
    },
    {
      "name": "SVM-RBF",
      "class": "SVMModel",
      "parameters": {
        "random_state": 42,
        "scale_features": true,
        "kernel": "rbf",
        "C": 1.0
      }
    },
    {
      "name": "SVM-LINEAR",
      "class": "SVMModel",
      "parameters": {
        "random_state": 42,
        "scale_features": true,
        "kernel": "linear",
        "C": 1.0
      }
    },
    {
      "name": "RandomForest",
      "class": "RandomForestModel",
      "parameters": {
        "random_state": 42,
        "scale_features": false,
        "n_estimators": 50,
        "max_depth": 5
      }
    },
    {
      "name": "GradientBoosting",
      "class": "GradientBoostingModel",
      "parameters": {
        "random_state": 42,
        "scale_features": false,
        "n_estimators": 50,
        "max_depth": 3
      }
    },
    {
      "name": "NaiveBayes",
      "class": "NaiveBayesModel",
      "parameters": {
        "random_state": 42,
        "scale_features": false
      }
    },
    {
      "name": "MLP",
      "class": "MLPModel",
      "parameters": {
        "random_state": 42,
        "scale_features": true,
        "hidden_layer_sizes": [
          50
        ],
        "max_iter": 500
      }
    }
  ],
  "ml_metrics": [
    "accuracy",
    "f1"
  ],
  "cv_folds": 5,
  "correlation_target": "best_accuracy",
  "plots": [
    "LINE_PLOT_COMPLEXITY_COMBINED",
    "DATASETS_OVERVIEW",
    "COMPLEXITY_CORRELATIONS"
  ],
  "run_mode": "complexity",
  "pairwise_distance_measures": [
    "Pearson r",
    "Spearman \u03c1",
    "Kendall \u03c4",
    "Mutual Information",
    "Euclidean Distance",
    "Distance Correlation",
    "Cosine Similarity",
    "Manhattan Distance"
  ]
}