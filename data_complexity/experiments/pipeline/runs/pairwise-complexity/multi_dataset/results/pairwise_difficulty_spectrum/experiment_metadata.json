{
  "experiment_name": "pairwise_difficulty_spectrum",
  "datasets": [
    {
      "label": "Easy Gaussian",
      "type": "Gaussian",
      "params": {
        "num_samples": 400,
        "train_size": 0.5,
        "equal_test": true,
        "class_separation": 6.0,
        "cov_type": "spherical",
        "cov_scale": 0.5
      }
    },
    {
      "label": "Medium Gaussian",
      "type": "Gaussian",
      "params": {
        "num_samples": 400,
        "train_size": 0.5,
        "equal_test": true,
        "class_separation": 3.0,
        "cov_type": "spherical",
        "cov_scale": 1.0
      }
    },
    {
      "label": "Hard Gaussian",
      "type": "Gaussian",
      "params": {
        "num_samples": 400,
        "train_size": 0.5,
        "equal_test": true,
        "class_separation": 1.0,
        "cov_type": "spherical",
        "cov_scale": 2.0
      }
    },
    {
      "label": "Easy Moons",
      "type": "Moons",
      "params": {
        "num_samples": 400,
        "train_size": 0.5,
        "equal_test": true,
        "moons_noise": 0.05
      }
    },
    {
      "label": "Hard Moons",
      "type": "Moons",
      "params": {
        "num_samples": 400,
        "train_size": 0.5,
        "equal_test": true,
        "moons_noise": 0.5
      }
    },
    {
      "label": "Circles",
      "type": "Circles",
      "params": {
        "num_samples": 400,
        "train_size": 0.5,
        "equal_test": true,
        "circles_noise": 0.05
      }
    },
    {
      "label": "XOR",
      "type": "XOR",
      "params": {
        "num_samples": 400,
        "train_size": 0.5,
        "equal_test": true
      }
    }
  ],
  "x_label": "Dataset",
  "ml_models": [
    {
      "name": "LogisticRegression",
      "class": "LogisticRegressionModel",
      "parameters": {
        "random_state": 42,
        "scale_features": true,
        "max_iter": 1000
      }
    },
    {
      "name": "KNN-5",
      "class": "KNNModel",
      "parameters": {
        "random_state": 42,
        "scale_features": true,
        "n_neighbors": 5
      }
    },
    {
      "name": "KNN-3",
      "class": "KNNModel",
      "parameters": {
        "random_state": 42,
        "scale_features": true,
        "n_neighbors": 3
      }
    },
    {
      "name": "DecisionTree",
      "class": "DecisionTreeModel",
      "parameters": {
        "random_state": 42,
        "scale_features": false,
        "max_depth": 5
      }
    },
    {
      "name": "SVM-RBF",
      "class": "SVMModel",
      "parameters": {
        "random_state": 42,
        "scale_features": true,
        "kernel": "rbf",
        "C": 1.0
      }
    },
    {
      "name": "SVM-LINEAR",
      "class": "SVMModel",
      "parameters": {
        "random_state": 42,
        "scale_features": true,
        "kernel": "linear",
        "C": 1.0
      }
    },
    {
      "name": "RandomForest",
      "class": "RandomForestModel",
      "parameters": {
        "random_state": 42,
        "scale_features": false,
        "n_estimators": 50,
        "max_depth": 5
      }
    },
    {
      "name": "GradientBoosting",
      "class": "GradientBoostingModel",
      "parameters": {
        "random_state": 42,
        "scale_features": false,
        "n_estimators": 50,
        "max_depth": 3
      }
    },
    {
      "name": "NaiveBayes",
      "class": "NaiveBayesModel",
      "parameters": {
        "random_state": 42,
        "scale_features": false
      }
    },
    {
      "name": "MLP",
      "class": "MLPModel",
      "parameters": {
        "random_state": 42,
        "scale_features": true,
        "hidden_layer_sizes": [
          50
        ],
        "max_iter": 500
      }
    }
  ],
  "ml_metrics": [
    "accuracy",
    "f1"
  ],
  "cv_folds": 5,
  "correlation_target": "best_accuracy",
  "plots": [
    "LINE_PLOT_COMPLEXITY_COMBINED",
    "DATASETS_OVERVIEW",
    "COMPLEXITY_CORRELATIONS"
  ],
  "run_mode": "complexity",
  "pairwise_distance_measures": [
    "Pearson r",
    "Spearman \u03c1",
    "Kendall \u03c4",
    "Mutual Information",
    "Euclidean Distance"
  ]
}